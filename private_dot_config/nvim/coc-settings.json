{
  "languageserver": {
    "solargraph": {
      "command": "solargraph",
      "args": ["--stdio"],
      "filetypes": ["ruby"],
      "initializationOptions": {
        "storagePath": "/tmp/solargraph"
      }
    }
  },
  "coc.preferences.rootPatterns": [".gitignore", ".env", ".git"],
  "explorer.file.reveal.auto": false,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "vim-devicons",
  "explorer.keyMappings.global": {
    "n": "addFile",
    "N": "addDirectory",
    "d": "delete",
    "r": "rename",
    "f": "search",
    "F": "searchRecursive",
    "p": "pasteFile"
  },
  "explorer.width": 30,
  "http.proxyStrictSSL": false,
  "solargraph.commandPath": "/Users/takatsugu.yoneya/.anyenv/envs/rbenv/shims/solargraph",
  "solargraph.useBundler": false,
  "suggest.enablePreselect": true,
  "tsserver.enableJavascript": true
}
